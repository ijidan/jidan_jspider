        $(".ask_submit").on("click",function () {
            var name = $("input[name='ask_name']").val();
            var code = $("select[name='ask_country_code']").val();
            var mobile = $("input[name='ask_mobile']").val();
            var email = $("input[name='ask_email']").val();
            console.log(name)
            if(!name) {
                $(".tips-name").show();
                return false;
            }
            if(!mobile) {
                $(".tips-mobile").show();
                return false;
            }
            if(email) {
                var reg = /^\w+((.\w+)|(-\w+))@[A-Za-z0-9]+((.|-)[A-Za-z0-9]+).[A-Za-z0-9]+$/; //正则表达式
                if(!reg.test(email)) {
                    $(".tips-email").show();
                    return false;
                }
            }
            //判断是86的时候必须输入11为手机号
            if(code == 86) {
                if(mobile.length !== 11) {
                    layer.msg('请输入正确的11位手机号');
                    return false
                }
            }
            $(".ask_price_pup").hide();
            layer.load(2)

            var param = new Object();
            param._token = "gJGyad1AhdHLr7ZtkurK310aTh921AlArevtnVzc";
            param.name=name;
            param.code=code;
            param.mobile=mobile;
            param.email = email;
            param.house_id = "563940838"
            param.type = 1;
            param.uoolu = getQueryString('uoolu');
            param.hmsr = getQueryString('hmsr');
            param.full_url = 'https://www.uoolu.com/house-563940838.html?uuid=31b30d18-1e83-41ac-a8fd-b71c10345f92';
            $.post('/house/detailSignUp',param,function(re){
                layer.closeAll('loading');
                if(re.code=='100'){
                    $(".ask_price_pup").show();
                    $(".ask-form").hide();
                    $(".ask-success-popup").show();
                }else{
                    layer.msg('失败');
                }
            });
        });

        $("input").on("keyup", function() {
            $(".tips").hide();
        });

        //入口函数
        $(function(){
            $(".subbtn").on("click",function (){
                console.log(444)
                var name = $('.username2').val();
                var email = $('.email2').val();
                var tel = $('.phonenum2').val();
                var msg = $('.leavemsg').val();
                var code = $('.phone2 span').html();

                if(name == ''){
                    layer.msg('姓名不可为空');
                    return;
                }

                if(email == ''){
                    layer.msg('邮箱不可为空');
                    return;
                }

                if(email) {
                    var reg = /^\w+((.\w+)|(-\w+))@[A-Za-z0-9]+((.|-)[A-Za-z0-9]+).[A-Za-z0-9]+$/; //正则表达式
                    if(!reg.test(email)) {
                        layer.msg('邮箱格式不准确');
                        return false;
                    }
                }

                if(tel == ''){
                    layer.msg('手机号不可为空');
                    return;
                }

                if(code == '+86' ){
                    if(tel.length != 11){
                        layer.msg('手机号长度应为11位');
                        return;
                    }
                }

                var param = new Object();
                param._token = "gJGyad1AhdHLr7ZtkurK310aTh921AlArevtnVzc";
                param.name=name;
                param.email=email;
                param.tel=tel;
                param.msg=msg;
                param.code=code;
                param.house_id = "563940838"
                param.is_appoint = "0"

                console.log(param)
                layer.load(2);
                $.post('/house/consult',param,function(re){
                    layer.closeAll('loading');
                    if(re.code=='100'){
                        layer.msg('预约成功');
                        var name = $('.username2').val('');
                        var email = $('.email2').val('');
                        var tel = $('.phonenum2').val('');
                        var msg = $('.leavemsg').val('');
                        var code = $('.phone2').html(`<span>+86</span>`);
                    }else{
                        layer.msg(re.msg);
                    }
                });

            });


        })

        $(".kan_submit").on("click",function () {
            var name = $("input[name='kan_name']").val();
            var code = $("select[name='kan_country_code']").val();
            var mobile = $("input[name='kan_mobile']").val();
            var email = $("input[name='kan_email']").val();
            if(!name) {
                $(".tips-name-kan").show();
                return false;
            }
            if(!mobile) {
                $(".tips-mobile-kan").show();
                return false;
            }
            if(email) {
                var reg = /^\w+((.\w+)|(-\w+))@[A-Za-z0-9]+((.|-)[A-Za-z0-9]+).[A-Za-z0-9]+$/; //正则表达式
                if(!reg.test(email)) {
                    $(".tips-email-kan").show();
                    return false;
                }
            }

            //判断是86的时候必须输入11为手机号
            if(code == 86) {
                if(mobile.length !== 11) {
                    layer.msg('请输入正确的11位手机号');
                    return false
                }
            }
            $(".kan_tuan_pup").hide();
            layer.load(2)

            var param = new Object();
            param._token = "gJGyad1AhdHLr7ZtkurK310aTh921AlArevtnVzc";
