                var url = $(this).attr('url');
                if(!url)
                {
                    return false;
                }
                layer.open({
                    type: 2,
                    title: '全景看房',
                    shadeClose: true,
                    shade: 0.8,
                    area: ['100%', '100%'],
                    content: url
                });
            });
            var co = getCookies("house-detail-pop");
            if(co&&co=='close'){
                $('.flat_content').hide();
            }
            else
            {
                $('.flat_content').show();
            }
            setCookies('house-detail-pop','close','.uoolu.com');

            /*if(Storage.getItem("house_detail_flat_pop")==100)
            {
                $('.flat_content').hide();
            }
            else
            {
                $('.flat_content').show();
            }
            Storage.setItem('house_detail_flat_pop',100);*/


