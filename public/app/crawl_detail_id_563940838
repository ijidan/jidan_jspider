    var storageName = 'uooluHistory';
    var options = {
        maxNum:10,
        apiSettings: {
            url: "/house/search/ajax?_token=gJGyad1AhdHLr7ZtkurK310aTh921AlArevtnVzc"
        },
        storageName: storageName,
        showHistory:true,
        noResultsText:'没有匹配到内容，请更换您的关键字',
        onSearch: function(data) {
            if (data && data.url) {
                console.log(data.house);
                window.location.href = data.url;
            }
            console.log(data);
        },
    };
    var uooluSeach = $('#houseListSearch').search(options);


    // 搜索按钮点击事件
    $('#searchBtn').on('click',function(){

        var searchText = $('#searchTextInput').val();
        // 设置历史记录到local storage
        // storage名称与上面的配置一致

        var uooluHistoryList = localStorage.getItem(storageName)? JSON.parse(localStorage.getItem(storageName)) : [];

        if(searchText && uooluHistoryList.indexOf(searchText)===-1){
            uooluHistoryList.unshift(searchText);
            localStorage.setItem(storageName, JSON.stringify(uooluHistoryList));
        }

        if(searchText){
            window.location.href="/house?key="+searchText;
        }
    })


    var check_phone_click=true;
    //读取cookie
    function getCookies(name)
    {
        var arr = document.cookie.match(new RegExp("(^| )"+name+"=([^;]*)(;|$)"));
        if(arr != null) return unescape(arr[2]); return null;
    }
    //删除cookie
    function delCookies(name,domain)
    {
        var exp = new Date();
        exp.setTime(exp.getTime() - 1);
        var cval=getCookies(name);
        if(cval){
            document.cookie=name +"="+" "+";expires="+exp.toGMTString()+";path=/;domain="+domain;
        }
    }
    //写入cookie
    function setCookies(name,value,domain)
    {
        var Days = 1;
        var expdate   = new Date();
        expdate.setTime(expdate.getTime() + Days*24*60*60*1000);
        document.cookie = name + "="+ escape(value) +";expires="+ expdate.toGMTString()+";path=/;domain="+domain;
    }
    $(function(){
        //new_header_start

        $(".login-status__logged").mouseover(function(){
            $(".drop_list_wrap").removeClass('dn');
        }).mouseout(function(){
            $(".drop_list_wrap").addClass('dn');
        });



        $(".mod-filter-globe").mouseover(function(){
            $(this).addClass('active');
        }).mouseout(function(){
            $(this).removeClass('active');
        });
        $(".nav-item").mouseover(function(){
            $(this).addClass('active');
            $(this).addClass('actives');
        }).mouseout(function(){
            if($(this).attr("data")!='购房'){
                $(this).removeClass('active');
            }
            $(this).removeClass('actives');
        });
        //new_header_end

        //new_quick_start
        $(".tool-item").click(function(){
            if(!$(this).hasClass('active')){
                $(".tool-item").removeClass("active");
                $(this).addClass('active');
            }else{
                $(this).removeClass('active');
                setCookies('event','close','.uoolu.com');
            }
        });



        $(".call_free").click(function(){
            if(check_phone_click==true){
                return false;
            }
            check_phone_click=false;
            var free_phone = $("input[name='free_phone']").val();
            if(!regex_mobile_cn.test(free_phone)){
                layer.msg('电话号码错误');
                return false;
            }
            $.post('/ajax/call_free',{free_phone:free_phone,_token:"gJGyad1AhdHLr7ZtkurK310aTh921AlArevtnVzc"},function(re){
                if(re.code==100){
                    layer.msg(re.msg);
                }
            });
            return false;
        });


        $(".tool-pop-close").click(function(e){
            $(this).parent().parent().parent().removeClass('active');
            setCookies('event','close','.uoolu.com');
            return false;
        });

        var co = getCookies("event");
        if(!co){
            $('.tool-mini').addClass('active');
        }

        //end_quick_end


        $(".event-hover").mouseover(function(){

            $(this).addClass('active');

        }).mouseout(function(){
            $(this).removeClass('active');
        });

        $('.event_nav').mouseover(function(){
            $(this).addClass('selected');
        }).mouseout(function(){
            $(this).removeClass('selected');
        }).find('.link_nav').click(function(){return false;});

        $('.event_drop').mouseover(function(){
            $('.event_drop_container').removeClass('dn');
        }).mouseout(function(){
            $('.event_drop_container').addClass('dn');
        });

        $(".l_number").keyup(function(){  //keyup事件处理
            $(this).val($(this).val().replace(/\D|^0/g,''));
        }).bind("paste",function(){  //CTR+V事件处理
            $(this).val($(this).val().replace(/\D|^0/g,''));
        }).css("ime-mode", "disabled");  //CSS设置输入法不可用

        $(".f_number").keyup(function(){
            $(this).val($(this).val().replace(/[^0-9.]/g,''));
        }).bind("paste",function(){  //CTR+V事件处理
            $(this).val($(this).val().replace(/[^0-9.]/g,''));
        }).css("ime-mode", "disabled"); //CSS设置输入法不可用

        $('#friend_link_more').on('click', function(){
            var $linkMore = $('#friend_link_hide');
            if ($linkMore.hasClass('active')) {
                $(this).html('展开&gt;');
                $linkMore.removeClass('active');
            }
            else {
                $(this).html('收起&gt;');
                $linkMore.addClass('active');
            }
        });



        function backToTopFun() {
            var st = $(document).scrollTop();
            var begin = 800;
            (st > begin) ? $('.event-400,.event-app,.event-wx,.event-top').show(): $('.event-400,.event-app,.event-wx,.event-top').hide();
            //侧边栏的展示
            var sidebar = 750;
            (st>sidebar)?$('.event_sidebar_tips').show():$('.event_sidebar_tips').hide();
        };

        $(window).on("scroll", backToTopFun);

        backToTopFun();

        $('.tool-backToTop').click(function(){
            $("html, body").animate({ scrollTop: 0 }, 120);
        });
            })

