            $('.event_tab a').click(function(){
                var _self = $(this);
                var class_name = _self.attr('data');
                if(_self.hasClass('active')){
                    return false;
                }
                _self.closest('.event_tab').find('a').each(function(){
                    $(this).removeClass('active');
                    var tab_content_class = $(this).attr('data');
                    if(class_name==tab_content_class){
                        $('.'+class_name).show();
                    }
                    else{
                        $('.'+tab_content_class).hide();
                    }
                });
                _self.addClass('active');
                return false;
            });


            $(".search-footer-tab .tab-item").each(function(){
                var tab = $(this);
                tab.mouseover(function(){
                    $(".country_houses").hide();
                    $(".country_houses.country_"+tab.attr('data')).show();
                    $(".search-footer-tab .tab-item").removeClass('active');
                    tab.addClass('active');
                })
            });

            $("a.event_maodian").click(function() {
                var top = $($(this).attr("href")).offset().top-90;
                $("html, body").animate({
                    scrollTop: top + "px"
                }, {
                    duration: 500,
                    easing: "swing"
                });
                return false;
            });

