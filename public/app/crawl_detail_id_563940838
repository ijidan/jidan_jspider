<script>
    $(function() {
        $(document).on('click','.prompt-close-btn',function(re){
            $(this).parent().hide();
            setCookies(window.location.host+'_ip_window','close','.uoolu.com');
            return false;
        })
        $(document).on('click','.prompt-btn',function(re){
            $(".prompt-close-btn").click()
            return false;
        })
        var co = getCookies(window.location.host+"_ip_window");
        var href = window.location.href;
        console.log(href);
        if(!co && href == "https://www.uoolu.com/"){
            $.get('/ajax_ip_locale',function(rs){
                if(rs.code == '200' && rs.html)
                {
                    $(".language-prompt").html(rs.html);
                    $(".language-prompt").show();
                }
                if(rs.code == 301) {
                    window.location.href=rs.url
                }
            })
        }
    })

    $('.cs-select').niceSelect();
    //搜索

    $(document).on('click',function(e){
        var isLink = $(e.target).closest('.country_icon')[0] ===$('.country_icon')[0];

        if(isLink){
            return;
        }

        var isSelf = $(e.target).closest('.bar-item-main_language')[0] ===$('.bar-item-main_language')[0];

        if(!isSelf ){
            $('.country_icon').parent().removeClass('active');
        }
    });

    $('.country_icon').on('click',function(e){

        $(this).parent().toggleClass('active')
    })
    $(".search-btn").click(function(){
        var input=$("#searchTextInput").val();
        window.location.href="https://www.uoolu.com/house/?key="+input;
    })
    $(".select-language-btn").click(function(){
        var country_lang=$('#country_locale').val();
        $.get('/ajax_locale',{lang:country_lang},function(re){  //切换国家
            if(re.code==100){
                console.log(re.url);
                if(re.url) {
                    window.location.href = re.url;
                }else {
                    window.location.reload();
                }
            }
        })

    })
