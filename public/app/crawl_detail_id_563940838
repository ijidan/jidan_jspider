<script>
    $('.freeIMColor').mouseenter(function () {
        $(this).css("background-color", '#0d6ec2')
    })
    $('.freeIMColor').mouseleave(function () {
        $(this).css("background-color", '#1683e2')
    })

    // $('.freeIMColor').on('mouseleave', function () {
    //     $(this).css("background-color", '#1683e2')
    // })



    var check = true
    $('.close-im').on('click',function(){
        $('.im-wapper').addClass('im-wapper-small')
    })
    $('.im-wapper').hover(function(){
        if (!check) {
            $(this).removeClass('im-wapper-small')
            check = true;
        }
    },function(){
        check = false;
    })

    $('.openIM').on('click', function () {
         var self=$(this);
         var staff_id=self.attr('staff_id')?self.attr('staff_id'):'';
         var link=window.location.href;
         var  house_id=$('.IMtag').attr('im_house_id')?$('.IMtag').attr('im_house_id'):'';
         //develop_id
         var  develop_id=$('.IMtag').attr('develop_id')?$('.IMtag').attr('develop_id'):'';
         var cook=getCookie_im('uoolu_alc');
         if(cook)
         {
             const newWindow = window.open('/im/loading.html');
             //请求账号密码存在cookie里
            _ajax(house_id,newWindow,link,staff_id,develop_id);
         }
         else
         {

             // $('.login_btn').click();
         }
    })
    if(!getCookie_im('uoolu_alc')) {
        $(".openIM").addClass('login_btn');
    }

    function _ajax(house_id,newWindow,link,staff_id,develop_id='')
    {
        $.ajax({
            url:'/get_seller_accid',
            data:{
                house_id:house_id,
                link:link,
                staff_id:staff_id,
                develop_id:develop_id
            },
            success:function(res)
            {
                var res=JSON.parse(res);
                if(res.code==0)
                {
                    $('.login_btn').click()
                }
                else if(res.code==100) {
                    if (res.data.crm_accid) {
                        setCookies_im('uid', res.data.crm_accid);
                    }
                    if (res.data.crm_token) {
                        setCookies_im('sdktoken', res.data.crm_token);
                    }

                    if (res.data.staff_accid) {
                        setCookies_im('staff_accid', res.data.staff_accid);
                    }

                    setCookies_im('is_seller', res.data.is_seller);
                    if(getCookie_im('staff_accid'))
                    {
                        newWindow.location.href = 'https://webim.uoolu.com/uooluweb/im/main.html'//正式
                        // newWindow.location = 'https://pre-www.uoolu.com/wangyi/uooluweb/im/main.html'//测试
                    }
                    else
                    {
                        _ajax(house_id,newWindow,link,staff_id);
                    }
                }
            },
            error: function(res) {
                alert("正在加载请稍后！");
            }
        })
    }
    function setCookies_im(name,value)
    {
        var Days = 1;
        var expdate   = new Date();
        expdate.setTime(expdate.getTime() + Days*24*60*60*1000);
        document.cookie = name + "="+ escape(value) +";expires="+ expdate.toGMTString()+";path=/;domain=.uoolu.com";
    }


    function getCookie_im(name)
    {
        var arr,reg=new RegExp("(^| )"+name+"=([^;]*)(;|$)");

        if(arr=document.cookie.match(reg))

            return unescape(arr[2]);
        else
            return null;
    }


        var level1_type = $('.level1-nav li:nth-child(1) a').attr('level1-type');
        var level2_type = $('.level2-nav li:nth-child(1) a').attr('level2-type');

        $(function () {
            $(`.${level1_type}${level2_type}`).show()
            $('.level1-nav a').mouseenter(function () {

                level1_type = $(this).attr('level1-type');
                $(this).parent().addClass('active')
                $(this).parent().siblings().removeClass('active')
                $('.level3-nav li').hide()
                if ($(this).hasClass('nolevel2')) {
                    $('.level2-nav ul').hide()
                    $(`.${level1_type}`).show()
                } else {
                    $('.level2-nav ul').show()
                    $(`.${level1_type}${level2_type}`).show()
                }
            })
            $('.level2-nav a').click(function () {
                level2_type = $(this).attr('level2-type');
                $(this).parent().addClass('active')
                $(this).parent().siblings().removeClass('active')
                $('.level3-nav li').hide()
                $(`.${level1_type}${level2_type}`).show()
            })
        })


</script>

    
