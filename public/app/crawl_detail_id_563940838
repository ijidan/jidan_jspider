                if(index === 5) {
                                        country_arr.push({"id":140,"name":"Uganda","person":"Ugandan","lang":"English","locale":"ug","short":"UGA","icon":"https:\/\/main-uoolu.uoolu.com\/master\/Uganda.png","country_icon":"en","url":"https:\/\/uganda.uoolu.com","is_hot":0,"iso_code":"ug","index":0,"h5_url":"http:\/\/uganda.m.uoolu.com","created_at":"2018-11-01 18:56:36","updated_at":"2018-12-30 00:05:38","deleted_at":null});
