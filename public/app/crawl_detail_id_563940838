                                        country_arr.push({"id":142,"name":"Zambia","person":"Zambian","lang":"English","locale":"zm","short":"ZMB","icon":"https:\/\/main-uoolu.uoolu.com\/master\/Zambia.png","country_icon":"en","url":"https:\/\/zambia.uoolu.com","is_hot":0,"iso_code":"zm","index":0,"h5_url":"http:\/\/zambia.m.uoolu.com","created_at":"2018-11-01 18:56:36","updated_at":"2018-12-30 00:05:38","deleted_at":null});
                                        country_arr.push({"id":143,"name":"Zimbabwe","person":"Zimbabwean","lang":"English","locale":"zw","short":"ZWE","icon":"https:\/\/main-uoolu.uoolu.com\/master\/Zimbabwe.png","country_icon":"en","url":"https:\/\/zimbabwe.uoolu.com","is_hot":0,"iso_code":"zw","index":0,"h5_url":"http:\/\/zimbabwe.m.uoolu.com","created_at":"2018-11-01 18:56:36","updated_at":"2018-12-30 00:05:38","deleted_at":null});
                                    }
            }

            //显示出分页
            var length = country_arr.length;
            if(length > 15) {
                $(".lng-country-pages").show();
                var num = Math.ceil(length/15);
                var str = '';
                for(var i=1;i<=num;i++) {
                    if((i)===page) {
                        str += '<li class="lng-country-page active">';
                    }else {
                        str += '<li class="lng-country-page">';
                    }

                    str += '<a href="javascript:;" class="link-block">'+i+'</a>'
                    str += '</li>'
                }
                $(".lng-country-pages").empty().append(str);
            }else{
                $(".lng-country-pages").hide();
            }

            //显示出国家
            var data = pagination(page,country_arr);
            var str = '';
            for(var i=0;i<data.length;i++) {
                str += '<li class="lng-country-item" locale="'+data[i].locale+'">';
                str += '<a href="javascript:;" class="link-block">';
                str += '<img class="lng-flag" src="'+data[i].icon+'" alt="">';
                str += '<span class="lng-country-name">';
                str += data[i].name;
                str += '</span>';
                str += '<span class="lng-country-language" style="font-size:12px;">';
                str += "("+data[i].lang+")";
                str += '</span></a></li>'
            }
            $(".lng-country-list").empty().append(str);
        }

        function pagination(pageNo, array, pageSize=15) {
            var offset = (pageNo - 1) * pageSize;
            return (offset + pageSize >= array.length) ? array.slice(offset, array.length) : array.slice(offset, offset + pageSize);
        }


        //分页点击
        $(document).on("click", ".lng-country-pages>li",function() {
            $(".lng-country-pages>li").removeClass("active");
            $(this).addClass("active");

            var page = parseInt($(this).children("a").html());
            var index = 'hot';
            $(".country_type_list>li").each(function(i) {
                if($(this).hasClass('active')) {
                    index = $(this).attr("index");
                }
            });
            getCountry(index,page);
        });

        //点击国家
        $(document).on("click",".lng-country-list>li", function() {
            var country_lang = $(this).attr("locale");//当前语种
            var coin = $(".currency-selection").attr("coin");//当前币种
            changeCountryCoin(country_lang,coin)
        });

        function changeCountryCoin(country_lang,coin){
            $.get('/ajax_locale',{lang:country_lang},function(re){
                if(re.code==100 && re.url){
                    window.location.href = re.url;
                }else{
                    window.location.reload();
                }
            })
        }
    })
</script>
