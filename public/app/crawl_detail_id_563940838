                        var _self = $(el);
                        var form = $("#order-template").tmpl();
                        _self.append(form);
                    }
                });
            });

            var validator = $("#book_form" ).validate({
                rules: {
                    name: {
                        required: true
                    },
                    tel:{
                        required: true,
                        mobileCN: true
                    },
                    email:{
                        email:true
                    }
                },
                messages:{
                    tel:{required:'请输入11位手机号',mobileCN:"请输入11位手机号"},
                    name:{required:"请输入您的称呼"},
                    email:{email:"请输入正确格式的email"}
                },
                errorPlacement:function(error,el){
                    $(el).closest('.section-group').find('.group-error').html(error.text());
                    $(el).closest('.section-group').find('.group-error').removeClass('dn');
                    return false;
                },
                success:function(error,el){
                    if(!$(el).closest('.section-group').find('.group-error').hasClass('dn')){
                        $(el).closest('.section-group').find('.group-error').addClass('dn');
                    }
                }
            });
            $('.event_submit').click(function(){
                $('#book_form').submit();
                return false;
            });
            $('#book_form').submit(function(){
                if(window.locked == 'locked')return false;
                window.locked = 'locked';
                var is_ok = true;
                is_ok = $(this).valid();
                if(!is_ok){
                    window.locked = '';
                    return false;
                }
                var params = $(this).serialize();
                $.post('/ajax/house',params,function(data){
                    if(data.code==100){
                        layer.open({
                            type: 1,
                            title: false,
                            closeBtn: 0,
                            shadeClose: true,
                            content: '',
                            offset: ['25%', (window.screen.width - 697) / 2],
                            shade: [0.8, '#000'],
                            success: function (el) {
                                var _self = $(el);
                                var form = $("#success-template").tmpl();
                                _self.append(form);
                            }
                        });
                        $('#book_form').reset();
                    }
                    else{
                        //提示错误
                        layer.msg(data.msg);
                    }
                },'json').done(function(){
                    window.locked = '';
                });
                return false;
            });

            $('.section-tab-hd a').click(function(){
                var _self = $(this);
                var class_name = _self.attr('data');
                if(_self.hasClass('active')){
                    return false;
                }
                _self.closest('.event_tab').find('a').each(function(){
                    $(this).removeClass('active');
                    var tab_content_class = $(this).attr('data');
                    if(class_name==tab_content_class){
                        $('.'+class_name).show();
                    }
                    else{
                        $('.'+tab_content_class).hide();
                    }
                });
                _self.addClass('active');
                return false;
            });

            //收藏
            $('.event_fav').click(function(){
                var _self = $(this);
                if(_self[0].hasAttribute('is_disabled')){return false;}
                _self.attr('is_disabled',true);
                var action_type = (1- parseInt(_self.attr('data-fav')));
                $.post('/ajax/house',{_token:$('#_token').val(),oper:'fav',house_id:563940838,action_type:action_type},function(data){
                    _self.removeAttr('is_disabled');
                    if(data.code==401){
                        //alert('请先登录');
                        login_box_form();

                        return false;
                    }
                    else if (data.code) {
                        if(_self.hasClass('active')){
                            _self.removeClass('active');
                            return false;
                        }
                        _self.addClass('active');
                    }
                });
            });
            //查看全部户型
            var _room_content = $('.event_room_content');
            _room_content.find('.event_show_more').click(function(){
                if($(this).hasClass('active'))
                {
                    $(this).removeClass('active');
                    $(this).find('a:first').attr('title','查看全部>>').text('查看全部>>');
                    _room_content.find('.room_hide').hide();
                }
                else
                {
                    $(this).addClass('active');
                    $(this).find('a:first').attr('title','收起<<').text('收起<<');
                    _room_content.find('.room_hide').show();
                }
            });

            //户型切换
            $('.title-arrow-right').click(function(){
                var current = $(this).attr('data');
                var max = $(this).attr('max');
                var next = parseInt(current)+1;
                $('.invest_room_'+current).hide();
                $('.invest_room_'+next).show();
                if(next==max){
                    $(this).hide();
                }
                $(".title-arrow-left").show().attr('data',next);
            });

            $(".title-arrow-left").click(function(){
                var current = $(this).attr('data');
                var prev = parseInt(current)-1;

                $('.invest_room_'+current).hide();
                $('.invest_room_'+prev).show();
                if(prev<0){
                    $(this).hide();
                }
                $(".title-arrow-right").attr('data',prev).show();
            });

            //贷款、全款切换
            $(".full_loan a").click(function(){
                var _self = $(this);
                var data=$(this).attr('data');
                if(_self.hasClass('active')){
                    return false;
                }
                if(data=='full_div'){
                    _self.closest('.cont-wrap').find('#full_div').show();
                    _self.closest('.cont-wrap').find('#loan_div').hide();
                    $('.full_loan a[data=full_div]').addClass('active');
                    $('.full_loan a[data=loan_div]').removeClass('active');
                }
                if(data=='loan_div'){
                    _self.closest('.cont-wrap').find('#full_div').hide();
                    _self.closest('.cont-wrap').find('#loan_div').show();
                    $('.full_loan a[data=loan_div]').addClass('active');
                    $('.full_loan a[data=full_div]').removeClass('active');
                }
            });

            var top = $('.event_aim').offset().top+$('.event_aim').height()+35;
            $(window).scroll(function(){
                if($(window).scrollTop()>top){
                    $(".subMenus").show();
                    //$(".event_content").css({"padding-top":"70px"});
                }else{
                    $(".subMenus").hide();
                    //$(".event_content").css({"padding-top":"20px"});
                }
            });
            if($(window).scrollTop()>top){
                $(".subMenus").show();

                //$(".event_content").css({"padding-top":"70px"});
            }else{
                $(".subMenus").hide();
                //$(".event_content").css({"padding-top":"20px"});
            }
            $('.subMenus').smint({
                'scrollSpeed' : 1000
            });
            // 初始化swiper
            var picBanner = new Swiper('#pic-banner', {
                direction: 'horizontal',
                loop: false,
                autoplay: false,
                nextButton: '#pic-banner .swiper-button-next',
                prevButton: '#pic-banner .swiper-button-prev',
                onSlideChangeEnd: function () {
                    picThumbnail.slideTo(picBanner.activeIndex);
                    $('#pic-thumbnail .swiper-slide').eq(picBanner.activeIndex).addClass('active').siblings().removeClass('active');
                }
            });
            var picThumbnail = new Swiper('#pic-thumbnail', {
                direction: 'horizontal',
                loop: false,
                autoplay: false,
                slidesPerView: 'auto',
                spaceBetween: 8
            });
            $('#pic-thumbnail .swiper-button-next').click(function(){
                var index = picBanner.activeIndex+1;
                swiper_index(index);
            });
            $('#pic-thumbnail .swiper-button-prev').click(function(){
                var index = picBanner.activeIndex-1;
                if(index>=0)
                {
                    swiper_index(index);
                }
            });
            $(".swiper-thumbnail").click(function(){
                picBanner.slideTo($(this).attr('data'),0);
                picThumbnail.slideTo($(this).attr('data'),0);
            });

            



            $('.event_booking').click(function(){
                var form = $("#guwen-template").tmpl();
                layer.open({
                    type: 1,
                    title: false,
                    closeBtn: 0,
                    shadeClose: true,
                    content: '',
                    skin: 'ask-success-layer',
                    offset:['25%',(window.screen.width-697)/2],
                    shade: [0.8, '#000'],
                    success: function(el) {
                        var _self = $(el);
                        _self.append(form);
                        var tel_input = _self.find('input[name=phone]');
                        var code_input = _self.find('input[name=code]');
                        //判断是否需要验证码
                        var old_tel = '';
                        var check = 1;
                        if(old_tel)
                        {
                            code_input.parent().hide();
                            check = 0;
                        }
                        tel_input.on('input',function(){
                            if(old_tel == $(this).val())
                            {
                                code_input.parent().hide();
                                check = 0;
                            }
                            else
                            {
                                code_input.parent().show();
                                check = 1;
                            }
                        });

                        $(el).find('.event_send').click(function(){
                            var _self = $(this);
                            if(_self.hasClass('event_clicked')){
                                return false;
                            }
                            _self.addClass('event_clicked');
                            var tel = tel_input.val();
                            if(!(regex_mobile_cn.test(tel))){
                                _self.removeClass('event_clicked');
                                $(el).find('.error_phone').show();
                                return false;
                            }
                            else{
                                $(el).find('.error_phone').hide();
                            }
                            var param = {};
                            param._token = "gJGyad1AhdHLr7ZtkurK310aTh921AlArevtnVzc";
                            param.phone = tel;
                            param.type = 12;
                            param.oper='code';
                            var sec = 60;
                            $.post('/activity_ajax',param,function(re){
                                is_btn_click=false;
                                if(re.code=='100'){
                                    layer.msg('发送成功');
                                    $(el).find(".code_sec").addClass('dn');
                                    $(el).find(".code_secs").removeClass('dn');
                                    var sec_pao = setInterval(function(){
                                        if(sec==0){
                                            clearInterval(sec_pao);
                                            $(el).find(".code_secs").addClass('dn');
                                            $(el).find(".code_sec").removeClass('dn');
                                        }else{
                                            $(el).find(".code_secs").html(--sec+'s后重新获取');
                                        }
                                    },1000);
                                }else{
                                    layer.msg(re.msg);
                                }
                            },'json').done(function(){
                                _self.removeClass('event_clicked');
                            });
                        });
                        $(el).find('.event_sub').click(function(){
                            var _self = $(this);
                            if(_self.hasClass('event_clicked')){
                                return false;
                            }
                            _self.addClass('event_clicked');
                            if(!(regex_mobile_cn.test(tel_input.val()))){
                                _self.removeClass('event_clicked');
                                $('.error_phone').show();
                                return false;
                            }else{
                                $('.error_phone').hide();
                            }
                            if(code_input.parent().is(':visible') && code_input.val().length==0){
                                _self.removeClass('event_clicked');
                                $('.error_code').show();
                                return false;
                            }
                            else{
                                $('.error_code').hide();
                            }
                            var param = new Object();
                            param._token = "gJGyad1AhdHLr7ZtkurK310aTh921AlArevtnVzc";
                            param.tel = tel_input.val();
                            param.code = 86;
                            param.country_code = 'CN';
                            param.sms_code=code_input.val();
                            param.oper='book_house';
                            param.house_id = "563940838";
                            param.from="";
                            param.check = check;
                            $.post('/ajax/house',param,function(re){
                                if(re.code==100){
//                                    var form = $("#success-template").tmpl();
//                                    layer.open({
//                                        type: 1,
//                                        title: false,
//                                        closeBtn: 0,
//                                        shadeClose: true,
//                                        content: '',
//                                        skin: 'ask-success-layer',
//                                        offset:['25%',(window.screen.width-697)/2],
//                                        shade: [0.8, '#000'],
//                                        success: function(el) {
//                                            $(el).append(form);
//                                        }
//                                    });
                                    layer_success(re.data,re.number);

                                }else{
                                    var form = $("#fail-template").tmpl();
                                    layer.open({
                                        type: 1,
                                        title: false,
                                        closeBtn: 0,
                                        shadeClose: true,
                                        content: '',
                                        skin: 'ask-success-layer',
                                        offset:['25%',(window.screen.width-697)/2],
                                        shade: [0.8, '#000'],
                                        success: function(el) {
                                            $(el).append(form);
                                            $(".info-desc").html(re.msg);
                                        }
                                    });
                                    return false;
                                }
                            },'json').done(function(){
                                _self.removeClass('event_clicked');
                            });
                        });
                    }
                });
            });
            //猜你喜欢切换
            $.each(['next','prev'],function(index,name){
                var _self = $('.event_guess_page');
                _self.find('.page-'+name).click(function(){
                    var current_num = parseInt(_self.find('.current-num').text());
                    var total_num = parseInt(_self.find('.total-num').text());
                    if(!_self.find('.page-'+name).hasClass('disabled'))
                    {
                        if(name == 'next')
                        {
                            var next_num = current_num+1;
                            var other_name = 'prev';
                            if(next_num>=total_num)
                            {
                                $(this).addClass('disabled');
                            }
                        }
                        else if(name == 'prev')
                        {
                            var next_num = current_num-1;
                            var other_name = 'next';
                            if(next_num<=1)
                            {
                                $(this).addClass('disabled');
                            }
                        }
                        $('.event_guess_content').find('.event_guess_list_'+next_num).show().siblings().hide();
                        _self.find('.current-num').text(next_num);
                        _self.find('.page-'+other_name).removeClass('disabled');
                    }
                });
            });

            function swiper_index(index)
            {
                picBanner.slideTo(index);
                $('#pic-banner .swiper-slide').eq(index).addClass('active').siblings().removeClass('active');
                picThumbnail.slideTo(index);
                $('#pic-thumbnail .swiper-slide').eq(index).addClass('active').siblings().removeClass('active');
            }
        });

        function layer_success(id,number){
            layer.open({
                type: 1,
                title: false,
                closeBtn: 0,
                shadeClose: true,
                content: '',
                offset: ['25%', (window.screen.width - 697) / 2],
                shade: [0.8, '#000'],
                success: function (el) {
                    var _self = $(el);
                    var form = $("#recommend-template").tmpl({number:number});
                    _self.append(form);
                    var price = '';
                    $(document).on('click', '.price_list', function() {
                        var li = $(this);
                        $(".price_list").removeClass('active');
                        if(li.hasClass('active')){
                            li.removeClass('active');
                            price='';
                        }else{
                            price = li.attr('data');
                            li.addClass('active');
                        }
                    });
                    var tenement=[];
                    $(document).on('click', '.tenement_list', function() {
                        var li = $(this);
                        if(li.hasClass('active')){
                            li.removeClass('active');
                        }else{
                            price = li.attr('data');
                            li.addClass('active');
                        }
                        $(_self).find('.tenement_list').each(function(){
                            var li = $(this);
                            if(li.hasClass('active')){
                                tenement.push(li.attr('data'));
                            }
                        })
                    });
                    $(document).on('click', '.submit_btn', function() {
                        var param = {};
                        param.price=price;
                        param.tenement=tenement.join(',');
                        param.content = $(form).find('input[name="content"]').val();
                        param._token='gJGyad1AhdHLr7ZtkurK310aTh921AlArevtnVzc';
                        param.id=id;
                        $.post('/house/subscribe/success',param,function(re){
                            layer.closeAll();
                        });
                    })
                }
            });
        }

        $(".tanchuan_bg_close_v2").on("click",function (){
            $(".tanchuan_bg").hide();
            document.cookie="tanchuang_bg_vw=true";
        });

        $(function(){
            // new
            //微信二维码滚动
            var wx_qrcode = {
                obj : $('.section-wx-new-code'),
                top : $('.section-wx-new-code').offset().top,
                height : $('.section-wx-new-code').height(),
            }
            $(window).scroll(function(){
                var window_top = $(window).scrollTop();
                var max_top = $('.col-2-left').offset().top + $('.col-2-left').height() - wx_qrcode.height;
                if(wx_qrcode.top < window_top){
                    if(max_top > window_top){
                        wx_qrcode.obj.attr('style','position:fixed;top:70px;width:290px;');
                    }else{
                        wx_qrcode.obj.attr('style','position:relative;top:'+(max_top-wx_qrcode.top-12)+'px;');
                    }
                }else{
                    wx_qrcode.obj.removeAttr('style');
                }
            });
            // 最下方活动展示
            var close_float = getCookies('close_float');
            if(close_float)
            {
                $('.float-bottom-wx').hide();
                $('.big-xuanfu').show();
            }else{
                $('.float-bottom-wx').show();
                $('.big-xuanfu').hide();
            }
            $('.close-float').on('click',function(){
                setCookies('close_float','close','uoolu.com');
                $('.float-bottom-wx').hide();
                $('.big-xuanfu').show();
            })
            $('.close-big-float').on('click',function(){
                if($(this).hasClass('close2')){
                    $('.big-xuanfu').hide();
                    return;
                }
                $('.big-xuanfu').hide();
                $('.small-xuanfu').show();
            });
            $('.small-xuanfu').on('click',function(){
                $('.big-xuanfu').show();
                $('.small-xuanfu').hide();
            })
            $('.js-show-float').on('click',function(){
                // $('.float-yuyue-wapper').show()
            })
            $('.js-close-yuye').on('click',function(){
                $('.float-yuyue-wapper').hide()
            })
            $('.close-float').click();
            // 预约咨询

            $('.get-code').click(function(){
                var _self = $(this);
                if(_self.hasClass('event_clicked')){
                    return false;
                }
                _self.addClass('event_clicked');
                var tel = $('.yuyue-tel').val();
                if(!(regex_mobile_cn.test(tel))){
                    _self.removeClass('event_clicked');
                    $('.yuyue_error_phone').show();
                    return false;
                }
                else{
                    $('.yuyue_error_phone').hide();
                }
                var param = {};
                param._token = "gJGyad1AhdHLr7ZtkurK310aTh921AlArevtnVzc";
                param.phone = tel;
                param.type = 12;
                param.oper='code';
                var sec = 60;
                $.post('/activity_ajax',param,function(re){
                    is_btn_click=false;
                    if(re.code=='100'){
                        layer.msg('发送成功');
                        $(".get-code").addClass('dn');
                        $(".get-code-succ").removeClass('dn');
                        var sec_pao = setInterval(function(){
                            if(sec==0){
                                clearInterval(sec_pao);
                                $(".get-code").removeClass('dn');
                                $(".get-code-succ").addClass('dn');
                            }else{
                                $(".get-code-succ").html(--sec+'s后重新获取');
                            }
                        },1000);
                    }else{
                        layer.msg(re.msg);
                    }
                },'json').done(function(){
                    _self.removeClass('event_clicked');
                });
            });


            $('.js-know').on('click',function(){
                $('.yuyue-tel').val('');
                $('.yuyue-code').val('');
                $('.from-wapper').show();
                $('.has-get').hide();
            })


            $('.btn-yuyue').click(function(){
                var _self = $(this);
                if(_self.hasClass('event_clicked')){
                    return false;
                }
                _self.addClass('event_clicked');
                var tel = $('.yuyue-tel').val();
                if(!(regex_mobile_cn.test(tel))){
                    _self.removeClass('event_clicked');
                    $('.yuyue_error_phone').show();
                    return false;
                }else{
                    $('.yuyue_error_phone').hide();
                }
                var code_input = $('.yuyue-code').val();
                if(!code_input || code_input.length==0){
                    _self.removeClass('event_clicked');
                    $('.yuyue_error_code').show();
                    return false;
                }
                else{
                    $('.yuyue_error_code').hide();
                }
                //判断是否需要验证码
                var old_tel = '';
                var checked = 1;
                if(old_tel)
                {
                    checked = 0;
                }
                if(old_tel == tel)
                {
                    checked = 0;
                }
                else
                {
                    checked = 1;
                }
                var param = new Object();
                param._token = "gJGyad1AhdHLr7ZtkurK310aTh921AlArevtnVzc";
                param.tel = tel;
                param.code = 86;
                param.country_code = 'CN';
                param.sms_code=code_input;
                param.oper='book_house';
                param.house_id = "563940838";
                param.from="";
                param.check=checked;

                $.post('/ajax/house',param,function(re){
                    console.log(re)
                    if(re.code==100){
                        $('.from-wapper').hide();
                        $('.has-get').show();
                    }else{
                        if(re.msg)
                        {
                            layer.msg(re.msg);
                        }else{
                            layer.msg('预约失败');
                        }
                        return false;
                    }
                },'json').done(function(){
                    _self.removeClass('event_clicked');
                });
            });


        })
