            <script>
            $('.section-bd-info tbody tr').css('filter', 'blur(4px)')
            $('.section-bd-info tbody tr:first-child').css('filter', 'blur(0px)')
            $('.table-tip').css('filter', 'blur(4px)')
            var height1 = $('.section-bd-info .info-bd-left tbody').height()
            var height2 = $('.section-bd-info .info-bd-right tbody').height()
            $('.section-bd-info').append(`
            <div class='mhzzc'>
                    <a class='mhmain login_btn' href='javascript:;'>
                        <b>登录</b> <span>免费获取费用明细</span>
                    </a>
                </div>
            `)
            if (height1 > height2) {
                $('.mhzzc').css('height', height1-40)
            } else {
                $('.mhzzc').css('height', height2-40)
            }
