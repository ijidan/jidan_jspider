    <!-- 底部浮窗 -->
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                
                                                                                                    
                                                                                                                                                                                                                                                                                                                                                            
            

    <script src="/house/rim_data/563940838/"></script>
    <script>
        var markers=[];
        var map_street,map;
        var infowindow_r=[];
        var index_id;

        $(".rim_type").click(function(){
            clear_marker();
            $(".sub_list").hide();
            $('.'+$(this).attr('data')).show();
            $(".rim_type").removeClass('active');
            $(this).addClass('active');
            $(".sub_list").removeClass('active');
            $($('.'+$(this).attr('data'))[0]).addClass('active');
            $(".list_rim").hide();
            if($('.list_'+$(this).attr('data')).length>0){
                $('.list_'+$(this).attr('data')).show();

                $('.list_'+$(this).attr('data')).each(function(){
                    create_market($(this).attr('id'));
                });
                $(".empty_list").hide();
            }else{
                $(".empty_list").show();
            }


        });
        $(".sub_list").click(function(){
            clear_marker();
            $(".sub_list").removeClass('active');
            $(this).addClass('active');
            $('.list_rim').hide();
            if($(".list_"+$(this).attr('data')).length==0){
                $(".empty_list").show();
            }else{
                $(".empty_list").hide();
                $(".list_"+$(this).attr('data')).show();

                $(".list_"+$(this).attr('data')).each(function(){
                    create_market($(this).attr('id'));
                })
            }
        });


        $(".map-panel-flod").click(function() {
            if ($(this).hasClass('active')) {
                $(this).removeClass('active');
                $(".map-panel").removeClass('map-panel-hide');
            } else {
                $(this).addClass('active');
                $(".map-panel").addClass('map-panel-hide');
            }
        })


        function map_icon(type,hover){
            if(hover){
                return new BMap.Icon('/images/map-icon-'+type+'-hover.png', new BMap.Size(25,35));
                /*return L.icon({
                    iconUrl: '/images/map-icon-'+type+'-hover.png'
                });*/
            }else{
                return new BMap.Icon('/images/map-icon-'+type+'.png', new BMap.Size(27,30));
               /* return L.icon({
                    iconUrl: '/images/map-icon-'+type+'.png'
                });*/
            }
        }


        function create_market(id){
            if(!marklist[id]){
                return ;
            }


            var marker = new BMap.Marker(new BMap.Point(marklist[id].lng,marklist[id].lat),{icon: map_icon(marklist[id].icon,0)});  // 创建标注
            map.addOverlay(marker);              // 将标注添加到地图中
            markers[id]= marker;
            markers[id].addEventListener('click',function(){

                for(var mar in markers){
                    markers[mar].setIcon(map_icon(marklist[mar].icon,0));
                }
                var imgStrig = '';
//                if(marklist[id].img){
//                    imgStrig = '<div class="avatar"><img src="'+marklist[id].img+'" alt=""></div>';
//                }
                var address = '';
                if(marklist[id].address){
                    address = '<div class="location-icon" style="white-space: inherit;">'+marklist[id].address+'</div>'
                }
                var contentString =  '<div id="content" class="map-location-media" style="position: relative;box-shadow: none;width: auto;padding:0">'+
                        imgStrig+
                        '<div class="info">'+
                        '<div class="location-label" style="white-space: inherit;">'+marklist[id].name+'</div>'+
                        address+
                        '</div>'+
                        '</div>';

                var opts = {
                    width : 0,     // 信息窗口宽度
                    height: 0,     // 信息窗口高度
                    maxWidth:100,
                    maxHeight:50,
                    title : address , // 信息窗口标题
                    enableMessage:true,//设置允许信息窗发送短息
                    message:contentString
                };
                var infoWindow = new BMap.InfoWindow("曼谷大学城学生公寓二期（火爆开售）", opts);  // 创建信息窗口对象
                map.openInfoWindow(infoWindow,markers[id].getPosition()); //开启信息窗口

                index_id=id;

                markers[id].setIcon(map_icon(marklist[id].icon,1));

            });
            markers[id].addEventListener('mouseover',function(){
                this.setIcon(map_icon(marklist[id].icon,1));
            });

            markers[id].addEventListener('mouseout',function(){
                if(index_id!=id){
                    this.setIcon(map_icon(marklist[id].icon,0));
                }
            });
        }
        $(".list_rim").click(function(){
            var id = $(this).attr('id');
            show_info(id);
        });

        function clear_marker(){
            if(markers.length>0){
                for(var i in markers){
                    if(markers[i]){
                        markers[i].remove();
                    }
                }
            }
        }
        function show_info(id){

            for(var mar in markers){
                markers[mar].setIcon(map_icon(marklist[mar].icon,0));
            }

            var imgStrig='';
//            if(marklist[id].img){
//                imgStrig = '<div class="avatar"><img src="'+marklist[id].img+'" alt=""></div>';
//            }
            var address = '';
            if(marklist[id].address){
                address='<p class="location-icon" style="white-space: inherit;">'+marklist[id].address+'</p>';
            }
            var contentString =  '<div id="content" class="map-location-media" style="position: relative;box-shadow: none;width: auto;padding:0">'+
                    imgStrig+
                    '<div class="info" style="width:300px;">'+
                    '<p class="location-label" style="white-space: inherit;">'+marklist[id].name+'</p>'+
                    address+
                    '</div>'+
                    '</div>';

            var opts = {
                width : 0,     // 信息窗口宽度
                height: 0,     // 信息窗口高度
                maxWidth:100,
                maxHeight:50,
                title : address , // 信息窗口标题
                enableMessage:true,//设置允许信息窗发送短息
                message:contentString
            };
            var infoWindow = new BMap.InfoWindow("曼谷大学城学生公寓二期（火爆开售）", opts);  // 创建信息窗口对象
            map.openInfoWindow(infoWindow,markers[id].getPosition()); //开启信息窗口

            index_id=id;
            map.setView(markers[id].getPosition());
            markers[id].setIcon(map_icon(marklist[id].icon,1));

        }

        var latitude = '14.030903';
        var longitude = '100.614242';

        var point = new BMap.Point(longitude,latitude);
        function Gmap(point) {

           /* L.mapbox.accessToken = 'pk.eyJ1IjoibHVjaGFvYm8iLCJhIjoiY2pjazh0YmI3MDk4aDJxcGk4M2NiM3FzZCJ9.r19fgSGYQFJIGOAJkjzrzw';//'pk.eyJ1IjoidW9vbHUiLCJhIjoiY2o3dnFiNmluNHN0bTJ3cGswaHNmaGhzMCJ9.x8Df7E73LBiriRlx6TDelg';
            map = L.mapbox.map('locmap', 'mapbox.streets',{ zoomControl: false })
                    .setView(position, 15);
            //map.addLayer(L.mapbox.featureLayer('mapbox.streets'));
            //map_street = new google.maps.Map(document.getElementById("locmap_street"), initOptions);
            marker = L.marker(position, {icon: map_icon('location',1)}).addTo(map);

            marker.on('click',function(){
                infowindow=L.popup({maxWidth:'300'}).setLatLng(position).setContent("曼谷大学城学生公寓二期（火爆开售）").addTo(map);

            });
            new L.Control.Zoom({ position: 'bottomleft' }).addTo(map);

            map.scrollWheelZoom.disable();
            infowindow=L.popup({maxWidth:'300'}).setLatLng(position).setContent("曼谷大学城学生公寓二期（火爆开售）").addTo(map);
            if($(".list_business").length==0){
                $(".empty_list").show();
            }else{
                $('.list_business').each(function(){
                    create_market($(this).attr('id'));
                });
            }*/

            map = new BMap.Map("locmap");    // 创建Map实例
            map.centerAndZoom(point, 15);  // 初始化地图,设置中心点坐标和地图级别
            var marker = new BMap.Marker(point,{icon: map_icon('location',1)});  // 创建标注
            map.addOverlay(marker);              // 将标注添加到地图中

            var opts = {
                width : 0,     // 信息窗口宽度
                height: 0,     // 信息窗口高度
                maxWidth:50,
                title : "" , // 信息窗口标题
                enableMessage:true,//设置允许信息窗发送短息
                message:""
            };
            var infoWindow = new BMap.InfoWindow("曼谷大学城学生公寓二期（火爆开售）", opts);  // 创建信息窗口对象
            map.openInfoWindow(infoWindow,point); //开启信息窗口

            marker.addEventListener('click',function(){
                var infoWindow = new BMap.InfoWindow("曼谷大学城学生公寓二期（火爆开售）", opts);  // 创建信息窗口对象
                map.openInfoWindow(infoWindow,point); //开启信息窗口

            });

            map.disableScrollWheelZoom();     //开启鼠标滚轮缩放
            if($(".list_business").length==0){
                $(".empty_list").show();
            }else{
                $('.list_business').each(function(){
                    create_market($(this).attr('id'));
                });
            }

        }
        Gmap(point);

        $('.event_map_type').find('a').click(function(){
            if($(this).hasClass('active')){return;}
            var map_type = $(this).attr('map_type');
            if(map_type=='SATELLITE'){//卫星地图

                map.setMapType(BMAP_SATELLITE_MAP);

            }
            else if(map_type=='ROADMAP'){//路网图
                map.setMapType(BMAP_NORMAL_MAP);
                //L.mapbox.tileLayer('mapbox.streets').addTo(map);
               // map.tileLayer('mapbox.satellite');
//                map.setMapTypeId(google.maps.MapTypeId.ROADMAP);
//                $('#locmap').show();
//                $('#locmap_street').hide();
            }
            else if(map_type=='StreetView'){
                map.setMapType(BMAP_PERSPECTIVE_MAP);
                //L.mapbox.styleLayer('mapbox://styles/mapbox/emerald-v8').addTo(map);
//                var panoramaOptions = { position: position };
//                panorama = new google.maps.StreetViewPanorama(document.getElementById("locmap_street"), panoramaOptions);
//                map_street.setStreetView(panorama);
//                $('#locmap_street').show();
//                $('#locmap').hide();
//                $('#locmap_street').css({'margin-left':'0px','margin-right':'-60px'});
            }
            else if(map_type=='HYBRID'){
                map.setMapType(BMAP_HYBRID_MAP);
                //L.mapbox.styleLayer('mapbox://styles/mapbox/satellite-streets-v9').addTo(map);
//                map.setMapTypeId(google.maps.MapTypeId.HYBRID);
//                $('#locmap').show();
//                $('#locmap_street').hide();
            }
            $('.event_map_type').find('a').each(function(){
                if($(this).attr('map_type')==map_type){
                    $(this).addClass('active');
                }
                else{
                    $(this).removeClass('active');
                }
            });
        });
    </script>
<script>
    function getQueryString(name) {
        let reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)", "i");
        let r = window.location.search.substr(1).match(reg);
        if (r != null) {
            return unescape(r[2]);
        };
        return null;
    }
    var hmsr = getQueryString('hmsr');
    if(hmsr) {
        document.cookie = "hmsr="+ escape(hmsr) +";path=/;domain=.uoolu.com";
    }
</script>
</body>
</html>
