    function _ajax(house_id,newWindow,link,staff_id,develop_id='')
    {
        $.ajax({
            url:'/get_seller_accid',
            data:{
                house_id:house_id,
                link:link,
                staff_id:staff_id,
                develop_id:develop_id
            },
            success:function(res)
            {
                var res=JSON.parse(res);
                if(res.code==0)
                {
                    $('.login_btn').click()
                }
                else if(res.code==100) {
                    if (res.data.crm_accid) {
                        setCookies_im('uid', res.data.crm_accid);
                    }
                    if (res.data.crm_token) {
                        setCookies_im('sdktoken', res.data.crm_token);
                    }

                    if (res.data.staff_accid) {
                        setCookies_im('staff_accid', res.data.staff_accid);
                    }

                    setCookies_im('is_seller', res.data.is_seller);
                    if(getCookie_im('staff_accid'))
                    {
                        newWindow.location.href = 'https://webim.uoolu.com/uooluweb/im/main.html'//正式
                        // newWindow.location = 'https://pre-www.uoolu.com/wangyi/uooluweb/im/main.html'//测试
                    }
                    else
                    {
                        _ajax(house_id,newWindow,link,staff_id);
                    }
                }
            },
            error: function(res) {
                alert("正在加载请稍后！");
            }
        })
    }
    function setCookies_im(name,value)
    {
        var Days = 1;
        var expdate   = new Date();
        expdate.setTime(expdate.getTime() + Days*24*60*60*1000);
        document.cookie = name + "="+ escape(value) +";expires="+ expdate.toGMTString()+";path=/;domain=.uoolu.com";
    }


    function getCookie_im(name)
    {
        var arr,reg=new RegExp("(^| )"+name+"=([^;]*)(;|$)");

        if(arr=document.cookie.match(reg))

            return unescape(arr[2]);
        else
            return null;
    }


