<script type="text/javascript">
    jq(document).ready(function () {
        jq("#calculator").click(function () {
            jq("#fuceng1").show();

            jq("#loadYearRate").val("");
        });
        var arrayusa = new Array("华美银行,4.2,huamei.jpg", "汇丰银行,3.8,huifeng.png", "国泰银行,4,guotai.jpg", "富国银行,4,fuguo.jpg");
        var arrayau = new Array("西太平洋银行,4.59,xitai.jpg", "国家银行,4.59,guojia.jpg", "联邦银行,4.59,lianbang.png", "汇丰银行,4.59,huifeng.png", "澳新银行,4.59,aoxin.jpg");
        var arrayca = new Array("皇家银行,2.1,huangjia.jpg");
        var arrayen = new Array("中国银行,3.89,zhongguo.jpg");
        var arrayja = new Array("中国银行,3.2,zhongguo.jpg");
        var arrayma=new Array("汇丰银行,4.45,huifeng.png","大华银行,4.65,dahua.jpg","中国银行,5.2,zhongguo.jpg");
        jq("#fangdaicountry").change(function () {
            var array_bank;
            var country = jq("#fangdaicountry").val();

            jq("#fangdaibank").html("<option>请选择银行</option>");

            switch (country) {
                case "美国":
                    array_bank = arrayusa;
                    break;
                case "澳洲":
                    array_bank = arrayau;
                    break;
                case "加拿大":
                    array_bank = arrayca;
                    break;
                case "英国":
                    array_bank = arrayen;
                    break;
                case "日本":
                    array_bank = arrayja;
                    break;
                case "马来西亚":
                    array_bank = arrayma;
                    break;
            };

            if (array_bank != null) {
                for (var i = 0; i < array_bank.length; i++) {
                    var _value = array_bank[i];
                    // alert(_value);
                    jq("#fangdaibank").append("<option value='" + _value.split(',')[1] + "," + _value.split(',')[2] + "'>" + _value.split(',')[0] + "</option>");
                }
            }
        });
        var preimg = "//static.soufunimg.com/ggcp/world/fangjia/image/"
        jq("#fangdaibank").change(function () {

            var bank = jq("#fangdaibank").val();
            if (bank != "请选择银行") {
                jq("#bankimage").show();
                jq("#loadYearRate").val(bank.split(",")[0]);
                jq("#bankimage").attr("src", preimg + bank.split(",")[1]);
            }
            else {

                jq("#bankimage").hide();
            }
        });

        jq("#loadYearRate").change(function () {
            jq("#lilv").val(jq("#loadYearRate").val());

            if (jq("#loadYearRate").val() == "") {
                jq("#lilv").attr("disabled", false);
            }
            else {
                jq("#lilv").attr("disabled", "disabled");
            }
            if (jq("#lilv").val() != "" && jq("#loadDate").val() != "") {
                if (CheckVal()) {
                    ext_total();
                }
            }
        });
        jq("#cal").click(function () {

            if (CheckVal()) {
                ext_total();
            }

        });

    });
    var loadMoney, loadDate, loadYearRate;
    function CheckVal() {
        loadMoney = jq("#loadMoney").val().replace(/(^\s*)/g, "");
        loadDate = jq("#loadDate").val().replace(/(^\s*)/g, "");
        loadYearRate = jq("#loadYearRate").val().replace(/(^\s*)/g, "");
        var regx = new RegExp("^(([0-9]+\.[0-9]*[1-9][0-9]*)|([0-9]*[1-9][0-9]*\.[0-9]+)|([0-9]*[1-9][0-9]*))$");
        var regMonth = /^[1-9]\d*$/;
        if (loadMoney == "") {
            alert("贷款金额不能为空");
            jq("#loadMoney").focus();
            return false;
        }
        if (!regMonth.test(loadMoney)) {
            alert("贷款金额不正确");
            jq("#loadMoney").focus();
            return false;
        }
        if (loadDate == "") {
            alert("贷款期限不能为空");
            jq("#loadDate").focus();
            return false;
        }
        if (!regMonth.test(loadDate)) {
            alert("贷款期限不正确");
            jq("#loadDate").focus();
            return false;
        }
        if (loadYearRate == "") {
            alert("年利率不能为空");
            jq("#lilv").focus();
            return false;
        }
        if (!regx.test(loadYearRate)) {
            alert("年利率不正确");
            jq("#lilv").focus();
            return false;
        }
        return true;
    }
    //本息还款的月还款额(参数: 年利率/贷款总额/贷款总月份)
    function getMonthMoney1(lilv, total, month) {
        var lilv_month = lilv / 12;//月利率
        return total * lilv_month * Math.pow(1 + lilv_month, month) / (Math.pow(1 + lilv_month, month) - 1);
    }
    function ext_total() {
        //jq("#loadDate").html('');
        //jq("#loadYearRate").html('');           
        var month = jq("#loadDate").val();
        var lilv = jq("#loadYearRate").val() / 100;
        //var lilv = jq("#loadYearRate").val() / 100;//得到利率   
        //var lilv = getlilv(fmobj.callilv_s.value, 1, fmobj.calyears_s.value);//得到利率
        //房款总额
        //var fangkuan_total = calPrice * fmobj.calarea_s.value;
        //贷款总额
        var daikuan_total = jq("#loadMoney").val();
        var daikuantotal = daikuan_total;
        //首期付款
        //var money_first = fangkuan_total - daikuan_total;
        //2.本息还款
        //月均还款
           
        var month_money1 = getMonthMoney1(lilv, daikuantotal, month);//调用函数计算
        //fmobj.month_money1.innerHTML = Math.round(month_money1 * 100) / 100 + "(元)";
        jq("#monthPayMoney").val(Math.round(month_money1));
        //还款总额
        var all_total1 = month_money1 * month;
        var all_total1 = Math.round(all_total1);
        all_total1 = Math.round(all_total1 * 100) / 100;
