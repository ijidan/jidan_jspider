                    break;
                case "加拿大":
                    array_bank = arrayca;
                    break;
                case "英国":
                    array_bank = arrayen;
                    break;
                case "日本":
                    array_bank = arrayja;
                    break;
                case "马来西亚":
                    array_bank = arrayma;
