    var loadMoney, loadDate, loadYearRate;
    function CheckVal() {
        loadMoney = jq("#loadMoney").val().replace(/(^\s*)/g, "");
        loadDate = jq("#loadDate").val().replace(/(^\s*)/g, "");
        loadYearRate = jq("#loadYearRate").val().replace(/(^\s*)/g, "");
        var regx = new RegExp("^(([0-9]+\.[0-9]*[1-9][0-9]*)|([0-9]*[1-9][0-9]*\.[0-9]+)|([0-9]*[1-9][0-9]*))$");
        var regMonth = /^[1-9]\d*$/;
        if (loadMoney == "") {
            alert("贷款金额不能为空");
            jq("#loadMoney").focus();
            return false;
        }
        if (!regMonth.test(loadMoney)) {
            alert("贷款金额不正确");
            jq("#loadMoney").focus();
            return false;
        }
        if (loadDate == "") {
            alert("贷款期限不能为空");
            jq("#loadDate").focus();
            return false;
        }
        if (!regMonth.test(loadDate)) {
            alert("贷款期限不正确");
            jq("#loadDate").focus();
            return false;
        }
        if (loadYearRate == "") {
            alert("年利率不能为空");
            jq("#lilv").focus();
            return false;
        }
        if (!regx.test(loadYearRate)) {
            alert("年利率不正确");
            jq("#lilv").focus();
            return false;
        }
        return true;
    }
    //本息还款的月还款额(参数: 年利率/贷款总额/贷款总月份)
    function getMonthMoney1(lilv, total, month) {
        var lilv_month = lilv / 12;//月利率
        return total * lilv_month * Math.pow(1 + lilv_month, month) / (Math.pow(1 + lilv_month, month) - 1);
    }
    function ext_total() {
        //jq("#loadDate").html('');
        //jq("#loadYearRate").html('');           
        var month = jq("#loadDate").val();
        var lilv = jq("#loadYearRate").val() / 100;
        //var lilv = jq("#loadYearRate").val() / 100;//得到利率   
