        //首期付款
        //var money_first = fangkuan_total - daikuan_total;
        //2.本息还款
        //月均还款
           
        var month_money1 = getMonthMoney1(lilv, daikuantotal, month);//调用函数计算
        //fmobj.month_money1.innerHTML = Math.round(month_money1 * 100) / 100 + "(元)";
        jq("#monthPayMoney").val(Math.round(month_money1));
        //还款总额
        var all_total1 = month_money1 * month;
        var all_total1 = Math.round(all_total1);
        all_total1 = Math.round(all_total1 * 100) / 100;
        jq("#totalRateMoney").val(Math.round((all_total1 - daikuan_total) * 100) / 100);
        
        _ub.city = '海外'; //所在城市（中文）（这个地方的城市取得是页面左上角那个地方的城市）
        _ub.biz = 'w'; //海外网‘w’
        _ub.location = 0; //方位 ，网通为0，电信为1，如果无法获取方位，记录0
        _ub.collect(29, { 'vwg.page': 'wnewspage', "vww.country": encodeURIComponent(jq("#fangdaicountry option:selected").text()), "vww.loanbank": encodeURIComponent(jq("#fangdaibank option:selected").text()), "vww.loanamount": daikuan_total, "vww.loantime": month, "vww.annualinterestrate": jq("#loadYearRate").val(), "vww.monthlyrepayment": Math.round(month_money1), "vww.paymentinterest": Math.round((all_total1 - daikuan_total) * 100) / 100 });
            
    }
