<script type="text/javascript">

    jq(document).ready(function () {


        jq("#nlstart").click(function () {
            var zijin = jq("#zijin").val().replace(/(^\s*)/g, "");
            var shouru = jq("#shouru").val().replace(/(^\s*)/g, "");
            var zhichu = jq("#zhichu").val().replace(/(^\s*)/g, "");
            var mianji = jq("#mianji").val().replace(/(^\s*)/g, "");
            var regx = new RegExp("^(([0-9]+\.[0-9]*[1-9][0-9]*)|([0-9]*[1-9][0-9]*\.[0-9]+)|([0-9]*[1-9][0-9]*))$");
            var regMonth = /^[1-9]\d*$/;

            if (zijin == "") {
                alert("请填写可用于购房的资金");
                jq("#zijin").focus();
                return false;
            }
            if (!regMonth.test(zijin)) {
                alert("资金金额不正确");
                jq("#zijin").focus();
                return false;
            }
            if (shouru == "") {
                alert("请填写家庭月收入");
                jq("#shouru").focus();
                return false;
            }
            if (!regMonth.test(shouru)) {
                alert("家庭月收入金额不正确");
                jq("#shouru").focus();
                return false;
            }
            if (zhichu == "") {
                alert("请填写预计家庭每月固定支出");
                jq("#zhichu").focus();
                return false;
            }
            if (!regMonth.test(zhichu)) {
                alert("家庭每月固定支出金额不正确");
                jq("#zhichu").focus();
                return false;
            }

            if (mianji == "") {
                alert("请填写计划购买的房屋使用面积");
                jq("#mianji").focus();
                return false;
            }
            if (!regMonth.test(mianji)) {
                alert("房屋使用面积不正确");
                jq("#mianji").focus();
                return false;
            }


            //您可购买的房屋总价=（家庭月收入-家庭月固定支出）×( (（1＋月利率）＾还款月数)－1  )÷［月利率×（1＋月利率）＾还款月数］+持有资金 
            var month = parseInt(jq("#nianxian").val());
            var year = parseInt(month / 12);
            var lilu = 0.00576;
            if (year > 5)
                lilu = 0.00594;
            js00 = parseFloat(zijin); //现持有
            js01 = parseFloat(shouru); //月收入
            js02 = parseFloat(zhichu); //月支出
            js03 = parseFloat(mianji); //面积

            var d1 = js01 - js02;
            var d2 = Math.pow(1 + lilu, month) - 1;
            var d3 = lilu * Math.pow(1 + lilu, month)

            jq("#zongjia").val(Math.round(((d1 * d2) / d3) + js00));
            jq("#danjia").val(parseFloat(Math.round(((d1 * d2) / d3) + js00)) / js03);
            
            _ub.city = '菲律宾'; //所在城市（中文）（这个地方的城市取得是页面左上角那个地方的城市）
            _ub.biz = 'w'; //海外网‘w’
            _ub.location = 0; //方位 ，网通为0，电信为1，如果无法获取方位，记录0
            _ub.collect(50, { 'vwg.page': 'wnewspage', "vww.ownfunds": js00, "vww.monthlyincome": js01, "vww.monthlypay": js02, "vww.loantime": encodeURIComponent(jq("#nianxian").val()), "vww.area": js03, "vww.totalprice": (Math.round(((d1 * d2) / d3) + js00))/10000, "vww.unitprice": (parseFloat(Math.round(((d1 * d2) / d3) + js00)) / js03) });
        });

        jq("#touzistart").click(function () {
            var touzizongjia = jq("#touzizongjia").val().replace(/(^\s*)/g, "");
            var touzizujin = jq("#touzizujin").val().replace(/(^\s*)/g, "");

            var regx = new RegExp("^(([0-9]+\.[0-9]*[1-9][0-9]*)|([0-9]*[1-9][0-9]*\.[0-9]+)|([0-9]*[1-9][0-9]*))$");
            var regMonth = /^[1-9]\d*$/;

            if (touzizongjia == "") {
                alert("购房总价不能为空");
                jq("#touzizongjia").focus();
                return false;
            }
            if (!regMonth.test(touzizongjia)) {
                alert("购房总价金额不正确");
                jq("#touzizongjia").focus();
                return false;
            }
            if (touzizujin == "") {
                alert("每月租金费不能为空");
                jq("#touzizujin").focus();
                return false;
            }
            if (!regMonth.test(touzizujin)) {
                alert("每月租金费金额不正确");
                jq("#touzizujin").focus();
                return false;
            }

            jq("#touzijieguo").val(((touzizujin * 12) / touzizongjia * 100).toFixed(2));
            _ub.city = '菲律宾'; //所在城市（中文）（这个地方的城市取得是页面左上角那个地方的城市）
            _ub.biz = 'w'; //海外网‘w’
            _ub.location = 0; //方位 ，网通为0，电信为1，如果无法获取方位，记录0
            _ub.collect(30, { 'vwg.page': 'wnewspage', "vww.totalprice": touzizongjia/10000, "vww.rentprice": touzizujin, "vww.returnoninvestment": ((touzizujin * 12) / touzizongjia * 100).toFixed(2) });
        });

    });

</script>
