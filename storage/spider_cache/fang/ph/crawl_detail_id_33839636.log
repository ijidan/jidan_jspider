    function checkIDCard(input) {
        var re = /^\d{15}(\d{2}[A-Za-z0-9])?$/;
        if (!re.test(input)) {
            return false;
        }
        else {
            return true;
        }
    }
    //判断报名人数是否为数字
    function checkNumber(txtNumber) {
        var pat = /^[0-9]+$/;
        if (!pat.test(txtNumber)) {
            return false;
        }
        else {
            return true;
        }
    }
