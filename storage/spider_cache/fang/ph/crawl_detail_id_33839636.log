    var secs = 59;
    var wait = secs * 1000;
    function SendCheckcode(type) {
        if (V('txtPhone').value == '') {
            alert('手机号不能为空');
            V('txtPhone').focus();
            return false;
        }
        if (!validatemobile(V('txtPhone').value)) {
