        var arrayca = new Array("皇家银行,2.1,huangjia.jpg");
        var arrayen = new Array("中国银行,3.89,zhongguo.jpg");
        var arrayja = new Array("中国银行,3.2,zhongguo.jpg");
        var arrayma=new Array("汇丰银行,4.45,huifeng.png","大华银行,4.65,dahua.jpg","中国银行,5.2,zhongguo.jpg");
        jq("#fangdaicountry").change(function () {
