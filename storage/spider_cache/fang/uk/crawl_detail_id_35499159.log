    var loadMoney, loadDate, loadYearRate;
    function CheckVal() {
        loadMoney = jq("#loadMoney").val().replace(/(^\s*)/g, "");
        loadDate = jq("#loadDate").val().replace(/(^\s*)/g, "");
        loadYearRate = jq("#loadYearRate").val().replace(/(^\s*)/g, "");
        var regx = new RegExp("^(([0-9]+\.[0-9]*[1-9][0-9]*)|([0-9]*[1-9][0-9]*\.[0-9]+)|([0-9]*[1-9][0-9]*))$");
        var regMonth = /^[1-9]\d*$/;
