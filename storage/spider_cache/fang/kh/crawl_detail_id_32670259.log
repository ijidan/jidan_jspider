<script type="text/javascript">

    jq(document).ready(function () {


        jq("#nlstart").click(function () {
            var zijin = jq("#zijin").val().replace(/(^\s*)/g, "");
            var shouru = jq("#shouru").val().replace(/(^\s*)/g, "");
            var zhichu = jq("#zhichu").val().replace(/(^\s*)/g, "");
            var mianji = jq("#mianji").val().replace(/(^\s*)/g, "");
            var regx = new RegExp("^(([0-9]+\.[0-9]*[1-9][0-9]*)|([0-9]*[1-9][0-9]*\.[0-9]+)|([0-9]*[1-9][0-9]*))$");
            var regMonth = /^[1-9]\d*$/;

            if (zijin == "") {
                alert("请填写可用于购房的资金");
                jq("#zijin").focus();
                return false;
            }
            if (!regMonth.test(zijin)) {
                alert("资金金额不正确");
                jq("#zijin").focus();
                return false;
            }
            if (shouru == "") {
                alert("请填写家庭月收入");
                jq("#shouru").focus();
                return false;
            }
            if (!regMonth.test(shouru)) {
                alert("家庭月收入金额不正确");
                jq("#shouru").focus();
                return false;
            }
            if (zhichu == "") {
                alert("请填写预计家庭每月固定支出");
                jq("#zhichu").focus();
                return false;
            }
            if (!regMonth.test(zhichu)) {
                alert("家庭每月固定支出金额不正确");
                jq("#zhichu").focus();
                return false;
            }

            if (mianji == "") {
                alert("请填写计划购买的房屋使用面积");
                jq("#mianji").focus();
                return false;
            }
            if (!regMonth.test(mianji)) {
                alert("房屋使用面积不正确");
                jq("#mianji").focus();
                return false;
            }


            //您可购买的房屋总价=（家庭月收入-家庭月固定支出）×( (（1＋月利率）＾还款月数)－1  )÷［月利率×（1＋月利率）＾还款月数］+持有资金 
            var month = parseInt(jq("#nianxian").val());
            var year = parseInt(month / 12);
            var lilu = 0.00576;
            if (year > 5)
                lilu = 0.00594;
            js00 = parseFloat(zijin); //现持有
            js01 = parseFloat(shouru); //月收入
            js02 = parseFloat(zhichu); //月支出
            js03 = parseFloat(mianji); //面积

            var d1 = js01 - js02;
            var d2 = Math.pow(1 + lilu, month) - 1;
