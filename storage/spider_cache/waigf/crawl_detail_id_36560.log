            if (tag.length > 0) {
                var tags = decodeURIComponent(tag).split('|');
                var tagHtml = "";
                for (var i = 0; i < tags.length; i++) {
                    tagHtml += '<a href="/search/?t=newhouse&p=' + escape(tags[i]) + '" target="blank">' + tags[i] + '</a> / ';
                }
                $("#tag").html(tagHtml.substring(0, tagHtml.length - 3));
            }
            //sethistory("36560", "20190322/2019322153913842.jpg", "曼谷·暹罗豪庭拉差达 - Siamese Exclusive Ratchada","70.00","newhouse");

            var myCenter = new google.maps.LatLng(13.8009592, 100.57487040000001);

            function initialize() {
                var mapProp = {
                    center: myCenter,
                    zoom: 16,
                    mapTypeId: google.maps.MapTypeId.ROADMAP
                };

                var map = new google.maps.Map(document.getElementById("googleMap"), mapProp);

                var marker = new google.maps.Marker({
                    position: myCenter,
                });

                marker.setMap(map);

                var infowindow = new google.maps.InfoWindow({
                    content: "曼谷·暹罗豪庭拉差达 - Siamese Exclusive Ratchada"
                });

                infowindow.open(map, marker);
            }

            google.maps.event.addDomListener(window, 'load', initialize);

        </script>

</body>
</html>